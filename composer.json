{
    "name": "keyboardman/filesystem-bundle",
    "description": "Symfony bundle - File Storage API with Gaufrette (upload, rename, move, delete, multi-filesystems, cache)",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "symfony/config": "^6.4|^7.0",
        "symfony/dependency-injection": "^6.4|^7.0",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/http-foundation": "^6.4|^7.0",
        "symfony/http-kernel": "^6.4|^7.0",
        "symfony/routing": "^6.4|^7.0",
        "symfony/yaml": "^6.4|^7.0",
        "knplabs/gaufrette": "^0.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/browser-kit": "^6.4|^7.0",
        "symfony/css-selector": "^6.4|^7.0",
        "symfony/phpunit-bridge": "^6.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Keyboardman\\FilesystemBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keyboardman\\FilesystemBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
